{
  "name": "PersonalFinanceApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "jest e2e --detectOpenHandles",
    "test:e2e:android": "jest e2e --testNamePattern='Android'",
    "test:e2e:ios": "jest e2e --testNamePattern='iOS'",
    "appium": "appium server --port 4723",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:ios:debug": "cd ios && xcodebuild -workspace PersonalFinanceApp.xcworkspace -scheme PersonalFinanceApp -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 14' -derivedDataPath build"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.81.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.12.2",
    "react": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-date-picker": "^5.0.13",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "appium": "^3.0.2",
    "appium-uiautomator2-driver": "^5.0.3",
    "appium-xcuitest-driver": "^10.1.2",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3",
    "webdriverio": "^9.19.2"
  },
  "engines": {
    "node": ">=20"
  }
}
